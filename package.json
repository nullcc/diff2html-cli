{
  "name": "@nullcc/diff2html-cli",
  "version": "4.1.1",
  "homepage": "https://www.github.com/rtfpessoa/diff2html-cli",
  "description": "Fast Diff to colorized HTML",
  "keywords": [
    "git",
    "diff",
    "pretty",
    "side",
    "line",
    "side-by-side",
    "line-by-line",
    "character",
    "highlight",
    "pretty",
    "color",
    "html",
    "diff2html",
    "difftohtml",
    "colorized"
  ],
  "author": "Rodrigo Fernandes <rtfrodrigo@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtfpessoa/diff2html-cli.git"
  },
  "bugs": {
    "url": "https://www.github.com/rtfpessoa/diff2html-cli/issues"
  },
  "engines": {
    "node": "8.* || >=10"
  },
  "preferGlobal": true,
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "style": "npm run lint",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "coverage-html": "npm run coverage && open ./coverage/index.html",
    "codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "diff2html": "./bin/diff2html"
  },
  "main": "./build/main.js",
  "dependencies": {
    "@nullcc/diff2html": "^3.1.0",
    "clipboardy": "^2.1.0",
    "istanbul-lib-coverage": "^2.0.5",
    "istanbul-lib-source-maps": "^3.0.6",
    "open": "^6.4.0",
    "request": "^2.88.0",
    "source-map": "^0.7.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/nullcc__diff2html": "git+https://github.com/nullcc/diff2html-types.git",
    "@types/istanbul-lib-source-maps": "^1.2.2",
    "@types/jest": "24.0.18",
    "@types/node": "^12.7.2",
    "@types/request": "2.48.2",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "codacy-coverage": "^3.4.0",
    "eslint": "6.2.2",
    "eslint-config-prettier": "6.1.0",
    "eslint-config-standard": "14.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "typescript": "^3.5.3"
  },
  "license": "MIT",
  "files": [
    "bin",
    "build",
    "template.html"
  ],
  "bundleDependencies": [
    "@nullcc/diff2html",
    "@types/nullcc__diff2html"
  ]
}
